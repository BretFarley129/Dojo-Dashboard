<form action="/dojos/<%= @dojo['id'] %>/update" method="post">
    <input type="hidden" name="_method" value="patch">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <table>
        <tr>
            <td>Branch:</td>
            <td> <input type="text" value="<%= @dojo['branch'] %>" name="dojo[branch]"> </td>
        </tr>
        <tr>
            <td>Street:</td>
            <td> <input type="text" value="<%= @dojo['street'] %>" name="dojo[street]"> </td>
        </tr>
        <tr>
            <td>City:</td>
            <td> <input type="text" value="<%= @dojo['city'] %>" name="dojo[city]"> </td>
        </tr>
        <tr>
            <td>State:</td>
            <td> <input type="text" value="<%= @dojo['state'] %>" name="dojo[state]"> </td>
        </tr>
        <tr>
            <td></td>
            <td> <input type="submit" value="Update"> </td>
        </tr>
    </table>
</form>
<a href="/dojos">Home</a>|
<a href="/dojos/<%= @dojo['id'] %>">Show</a>
<% if @errors %>
    <% @errors.each do |error| %>
        <p style="color:red"><%= error%></p>
    <% end %>
<% end %>